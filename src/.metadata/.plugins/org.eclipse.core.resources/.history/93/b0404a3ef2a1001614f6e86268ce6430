package controller;

public class EscopoDeVariavel {
	public static void main(String[] args){
		int x = 5;
		System.out.println("x no início de main() é " +x);
		metodoA();
	}
	
		public static void metodoA(){
		int x = 25; 
		System.out.println("Variavel local x ao entrar no metodaA é " +x);
		x=x+1;
		System.out.println("Variavel local x antes de sair do metodaA é " +x);
		}
		
	}